{
  "name": "marumie-rssystem",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "prebuild": "bash scripts/decompress-data.sh",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "normalize": "python3 scripts/normalize_csv.py",
    "generate-structured": "tsx scripts/generate-structured-json.ts",
    "generate-preset": "tsx scripts/generate-preset-json.ts",
    "compress-data": "gzip -9 -k public/data/rs2024-structured.json",
    "setup": "npm install && npm run normalize && npm run generate-structured && npm run generate-preset"
  },
  "dependencies": {
    "@nivo/sankey": "^0.87.0",
    "next": "^15.1.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.4",
    "iconv-lite": "^0.6.3",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}